<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--页面顶部公共标题-->
    <declare-styleable name="BaseTitleView">
        <attr name="titleName" format="string" />
    </declare-styleable>
   <!-- 签约页面信息条目-->
    <declare-styleable name="SignItemView">
        <attr name="signItemTitle" format="string" />
        <attr name="signItemContent" format="string" />
    </declare-styleable>
    <!--语音波浪-->
    <declare-styleable name="WaveView">
        <attr name="speed" format="float" />
        <attr name="autoStart" format="boolean" />
        <attr name="amplitude" format="float" />
        <attr name="amplitudeFactor" format="float" />
        <attr name="standByAmplitude" format="float" />
        <attr name="ratio" format="float" />
        <attr name="frequency" format="float" />
    </declare-styleable>
<!--    <declare-styleable name="MainItemView-->
</resources>